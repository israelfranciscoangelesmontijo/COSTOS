#include<stdio.h>
#include<stdlib.h>
main(){
int PRO [5][12],PRE[5],x=0,y=0,z=0;
for(x=1;x<6;x++){

for(y=0;y<1;y++)
{

PRO [x][y],PRE[z];
printf("INGRESE EL PRECIO DEL PRODUCTO %d:",x,y);
scanf("%d",&PRO [x][y]);
}
}
for(x=1;x<6;x++){
for(y=0;y<1;y++)
{
printf("PRECIOS: \t %d",PRO [x][y]);
printf("\n");
}
}
}
